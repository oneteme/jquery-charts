{
  "name": "@oneteme/jquery-charts",
  "version": "0.0.26",
  "description": "jquery charts project",
  "private": false,
  "homepage": "https://github.com/oneteme/jquery-charts",
  "bugs": "https://github.com/oneteme/jquery-charts/issues",
  "author": {
    "name": "u$f",
    "url": "https://jquery.oneteme.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oneteme/jquery-charts.git"
  },
  "contributors": [
    {"name": "antonin77"},
    {"name": "NairodP"}
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "b1": "ng build @oneteme/jquery-highcharts",
    "cb1": "ng build \"@oneteme/jquery-core\" && ng build \"@oneteme/jquery-highcharts\"",
    "b2": "ng build @oneteme/jquery-apexcharts",
    "cb2": "ng build \"@oneteme/jquery-core\" && ng build \"@oneteme/jquery-apexcharts\"",
    "build": "ng build \"@oneteme/jquery-core\" && ng build \"@oneteme/jquery-apexcharts\" && ng build \"@oneteme/jquery-highcharts\" && ng build",
    "watch-core": "ng build \"@oneteme/jquery-core\" --watch --configuration development",
    "watch-b1": "ng build \"@oneteme/jquery-highcharts\" --watch --configuration development",
    "watch-b2": "ng build \"@oneteme/jquery-apexcharts\" --watch --configuration development",
    "dev1": "concurrently \"npm run watch-core\" \"wait-on dist/oneteme/jquery-core && npm run watch-b1\" \"wait-on dist/oneteme/jquery-highcharts && npm run start\"",
    "dev2": "concurrently \"npm run watch-core\" \"wait-on dist/oneteme/jquery-core && npm run watch-b2\" \"wait-on dist/oneteme/jquery-apexcharts && npm run start\"",
    "test": "ng test",
    "test:highcharts": "ng test \"@oneteme/jquery-highcharts\" --browsers=ChromeHeadless --watch=false"
  },
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "apexcharts": "^3.54.1",
    "highcharts": "^11.4.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular/cli": "~16.1.4",
    "@angular/compiler-cli": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.0.0",
    "typescript": "~5.1.6",
    "concurrently": "^9.1.2",
    "wait-on": "^8.0.2"
  }
}
